                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="phoneNumber" labelFloat="true" layoutX="10.0" layoutY="80.0" promptText="Telefonszám">
