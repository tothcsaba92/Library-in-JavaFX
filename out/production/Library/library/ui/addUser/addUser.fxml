               </VBox.margin>
            </JFXTextField>
            <HBox prefHeight="75.0" prefWidth="453.0">
               <children>
                  <JFXButton fx:id="addButton" onAction="#addUser" prefHeight="82.0" prefWidth="227.0" stylesheets="@addUser.css" text="Hozzáadás" />
                  <JFXButton fx:id="backButton" layoutX="10.0" layoutY="10.0" onAction="#backToMain" prefHeight="80.0" prefWidth="244.0" stylesheets="@addUser.css" text="Visszalépés" />
